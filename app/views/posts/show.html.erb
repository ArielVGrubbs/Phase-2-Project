<% if current_user %>
	<h1><%= @post.title %>
		<div class="specialsubtext">
			By: <%= link_to @post.user.name, user_path(@post.user) %>
		</div>
	</h1>
	<% if @post.image %>
		<img src=<%=@post.image%>></a><br><br>
	<% end %>
	<%= @post.body %><br><br><br>

	<h3>Comments:</h3>
	<% @post.comments.each do |comment| %>
		<div class="comments">
			<h4><%= link_to User.find_by(id: comment.user_id).username, user_path(User.find_by(id: comment.user_id)) %></h4>
			<%= comment.body %><br>
		</div>
	<% end %>

	<%= form_tag post_comments_path(@post.id), method: "post" do %>
		<div class="commentbody" >
			<%= text_area_tag :body %><br>
		</div>
		<%= submit_tag "Comment" %>
	<% end %>

	<p><%= @post.likes.count %> <%= (@post.likes.count) == 1 ? 'Like' : 'Likes'%></p>
	<% pre_like = @post.likes.find { |like| like.user_id == current_user.id} %>
	<% if pre_like %>
	<%= button_to 'Unlike', post_like_path(@post, pre_like), method: :delete %><br>
	<% else %>
	<%= button_to 'Like', post_likes_path(@post), method: :post %><br>
	<% end %>

	<div class="edit_delete">
	<div class="edit">
	<% if @post.user_id == current_user.id %>
		<form method="get" action='/posts/<%=@post.id%>/edit' class="button_to">
		<input value="Edit Post" type="submit" />
		</form>
	<% end %>
	</div>

	<% if current_user == @post.user %>
		<%= form_tag post_path(@post.id), method: "delete" do %>
			<%= submit_tag "Delete Post" %>
		<% end %> 
	<% end %>
	</div>

	</div>

					<!-- Footer -->
						<footer id="footer">
							<div class="inner">
								<section>
									<h2>Get in touch</h2>
									<form method="post" action="#">
										<div class="fields">
											<div class="field half">
												<input type="text" name="name" id="name" placeholder="Name" />
											</div>
											<div class="field half">
												<input type="email" name="email" id="email" placeholder="Email" />
											</div>
											<div class="field">
												<textarea name="message" id="message" placeholder="Message"></textarea>
											</div>
										</div>
										<ul class="actions">
											<li><input type="submit" value="Send" class="primary" /></li>
										</ul>
									</form>
								</section>
								<section>
									<h2>Follow</h2>
									<ul class="icons">
										<li><a href="#"><img src="data:image/png;base64," class="icon brands style2 fa-twitter"></a></li>
										<li><a href="#"><img src="data:image/png;base64," class="icon brands style2 fa-facebook-f"></a></li>
										<li><a href="#"><img src="data:image/png;base64," class="icon brands style2 fa-instagram"></a></li>
										<li><a href="#"><img src="data:image/png;base64," class="icon brands style2 fa-dribbble"></a></li>
										<li><a href="#"><img src="data:image/png;base64," class="icon brands style2 fa-github"></a></li>
										<li><a href="#"><img src="https://encrypted-tbn0.gstatic.com/images?q=tbn%3AANd9GcTXNUmHic8ZCQ388M9zTZoF4R5RduA4c6Qfaw&usqp=CAU" class="icon brands style2 fa-500px"></a></li>
										<li><a href="#"><img src="data:image/png;base64," class="icon solid style2 fa-phone"></a></li>
										<li><a href="#"><img src="data:image/png;base64," class="icon solid style2 fa-envelope"></a></li>
									</ul>
								</section>
								<ul class="copyright">
									<li>&copy; Untitled. All rights reserved</li><li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
								</ul>
							</div>
						</footer>

				</div>
<% else %>
	<h1>Please Sign In To View Posts</h1>

	<%= form_tag new_session_path, method: "get" do %>
		<%= submit_tag "Login" %>
	<% end %> 
<% end %>